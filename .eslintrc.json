{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "indent": ["error", 4],
        "eol-last": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "import/extensions": 0,
        "import/prefer-default-export": 0,
        "arrow-parens": 0,
        "comma-dangle": 0,
        "object-curly-newline": 0,
        "no-confusing-arrow": 0,
        "function-paren-newline": 0,
        "arrow-body-style": 0,
        "max-len": 0,
        "no-alert": 0,
        "no-use-before-define": 0,
        "no-restricted-syntax": 0,
        "implicit-arrow-linebreak": 0,
        "no-plusplus": 0
    }
}
